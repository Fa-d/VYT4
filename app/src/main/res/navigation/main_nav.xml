<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/dashBoardFragment">

    <fragment
        android:id="@+id/dashBoardFragment"
        android:name="dev.faddy.vyt1.fragments.dashboard.DashBoardFragment"
        android:label="fragment_dash_board"
        tools:layout="@layout/fragment_dash_board">
        <action
            android:id="@+id/action_dashBoardFragment_to_addNewPartyFragment"
            app:destination="@id/addNewPartyFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_itemsFragment"
            app:destination="@id/itemsFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_businessDashboardFragment"
            app:destination="@id/businessDashboardFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_paymentInFragment"
            app:destination="@id/paymentInFragment" />
    </fragment>
    <fragment
        android:id="@+id/drawerFragment"
        android:name="dev.faddy.vyt1.fragments.drawerComponents.DrawerFragment"
        android:label="fragment_drawer"
        tools:layout="@layout/fragment_drawer">

    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="dev.faddy.vyt1.fragments.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_partiesSettingsFragment"
            app:destination="@id/partiesSettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_itemsSettingsFragment"
            app:destination="@id/itemsSettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/partiesSettingsFragment"
        android:name="dev.faddy.vyt1.fragments.settings.PartiesSettingsFragment"
        android:label="fragment_parties_settings"
        tools:layout="@layout/fragment_parties_settings">
        <action
            android:id="@+id/action_partiesSettingsFragment_to_partyAddtionalFieldsFragment"
            app:destination="@id/partyAddtionalFieldsFragment" />
    </fragment>
    <fragment
        android:id="@+id/partyAddtionalFieldsFragment"
        android:name="dev.faddy.vyt1.fragments.settings.PartyAddtionalFieldsFragment"
        android:label="fragment_party_addtional_fields"
        tools:layout="@layout/fragment_party_addtional_fields" />
    <fragment
        android:id="@+id/partiesFragment"
        android:name="dev.faddy.vyt1.fragments.parties.PartiesFragment"
        android:label="fragment_parties"
        tools:layout="@layout/fragment_parties">

    </fragment>
    <fragment
        android:id="@+id/addNewPartyFragment"
        android:name="dev.faddy.vyt1.fragments.parties.addNewParty.AddNewPartyFragment"
        android:label="fragment_add_new_party"
        tools:layout="@layout/fragment_add_new_party" />
    <fragment
        android:id="@+id/transactionsFragment"
        android:name="dev.faddy.vyt1.fragments.transactions.TransactionsFragment"
        android:label="fragment_transactions"
        tools:layout="@layout/fragment_transactions" />
    <fragment
        android:id="@+id/itemsSettingsFragment"
        android:name="dev.faddy.vyt1.fragments.settings.ItemsSettingsFragment"
        android:label="fragment_items_settings"
        tools:layout="@layout/fragment_items_settings" />
    <fragment
        android:id="@+id/itemsFragment"
        android:name="dev.faddy.vyt1.fragments.items.ItemsFragment"
        android:label="ItemsFragment">

    </fragment>
    <fragment
        android:id="@+id/businessDashboardFragment"
        android:name="dev.faddy.vyt1.fragments.drawerComponents.BusinessDashboardFragment"
        android:label="fragment_business_dashboard"
        tools:layout="@layout/fragment_business_dashboard" />
    <fragment
        android:id="@+id/paymentInFragment"
        android:name="dev.faddy.vyt1.fragments.payment_in.PaymentInFragment"
        android:label="fragment_payment_in"
        tools:layout="@layout/fragment_payment_in" >
        <action
            android:id="@+id/action_paymentInFragment_to_addBankAccountFragment2"
            app:destination="@id/addBankAccountFragment2" />
        <action
            android:id="@+id/action_paymentInFragment_to_paymnetInSettingsFragment"
            app:destination="@id/paymnetInSettingsFragment" />
    </fragment>
    <dialog
        android:id="@+id/dashBoardBottomSheetFragment"
        android:name="dev.faddy.vyt1.fragments.dashboard.bottomsheets.DashBoardBottomSheetFragment"
        android:label="fragment_dash_board_bottom_sheet_list_dialog"
        tools:layout="@layout/fragment_dash_board_bottom_sheet_list_dialog" />
    <dialog
        android:id="@+id/paymentInBottomSheet"
        android:name="dev.faddy.vyt1.fragments.payment_in.bottomsheets.PaymentInBottomSheet"
        android:label="fragmet_payment_in_bottomsheet"
        tools:layout="@layout/fragmet_payment_in_bottomsheet" />
    <dialog
        android:id="@+id/filterPartyBottomSheetDialogFragment"
        android:name="dev.faddy.vyt1.fragments.parties.botttomsheet.FilterPartyBottomSheetDialogFragment"
        android:label="fragment_filter_party_bottom_sheet_dialog_list_dialog"
        tools:layout="@layout/fragment_filter_party_bottom_sheet_dialog_list_dialog" />
    <dialog
        android:id="@+id/cashSelectBottomSheet"
        android:name="dev.faddy.vyt1.fragments.payment_in.bottomsheets.CashSelectBottomSheet"
        android:label="CashSelectBottomSheet" >
    </dialog>
    <fragment
        android:id="@+id/addBankAccountFragment2"
        android:name="dev.faddy.vyt1.fragments.bankAccount.AddBankAccountFragment"
        android:label="fragment_add_bank_account"
        tools:layout="@layout/fragment_add_bank_account" />
    <dialog
        android:id="@+id/shareBottomSheet"
        android:name="dev.faddy.vyt1.fragments.payment_in.bottomsheets.ShareBottomSheet"
        android:label="ShareBottomSheet" />
    <fragment
        android:id="@+id/paymnetInSettingsFragment"
        android:name="dev.faddy.vyt1.fragments.payment_in.PaymnetInSettingsFragment"
        android:label="fragment_paymnet_in_settings"
        tools:layout="@layout/fragment_paymnet_in_settings" >
        <action
            android:id="@+id/action_paymnetInSettingsFragment_to_additionalChargesFragment"
            app:destination="@id/additionalChargesFragment" />
        <action
            android:id="@+id/action_paymnetInSettingsFragment_to_transportationDetailsFragment"
            app:destination="@id/transportationDetailsFragment" />
        <action
            android:id="@+id/action_paymnetInSettingsFragment_to_additionalFieldsFragment"
            app:destination="@id/additionalFieldsFragment" />
        <action
            android:id="@+id/action_paymnetInSettingsFragment_to_dueDatesAndPaymentTermsFragment"
            app:destination="@id/dueDatesAndPaymentTermsFragment" />
    </fragment>
    <fragment
        android:id="@+id/additionalChargesFragment"
        android:name="dev.faddy.vyt1.fragments.payment_in.AdditionalChargesFragment"
        android:label="fragment_additional_charges"
        tools:layout="@layout/fragment_additional_charges" />
    <fragment
        android:id="@+id/transportationDetailsFragment"
        android:name="dev.faddy.vyt1.fragments.payment_in.TransportationDetailsFragment"
        android:label="fragment_transportation_details"
        tools:layout="@layout/fragment_transportation_details" />
    <fragment
        android:id="@+id/additionalFieldsFragment"
        android:name="dev.faddy.vyt1.fragments.payment_in.AdditionalFieldsFragment"
        android:label="fragment_additional_fields"
        tools:layout="@layout/fragment_additional_fields" />
    <fragment
        android:id="@+id/dueDatesAndPaymentTermsFragment"
        android:name="dev.faddy.vyt1.fragments.payment_in.DueDatesAndPaymentTermsFragment"
        android:label="fragment_due_dates_and_payment_terms"
        tools:layout="@layout/fragment_due_dates_and_payment_terms" />
</navigation>